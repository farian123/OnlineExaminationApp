@model OnlineExaminationAppp.Models.ExamAttendViewModel
@{
    ViewBag.Title = "RunningExam";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>RunningExam</h2>
<div class="row">
    <div class="col-mid-3">
        <p class="text-left" id="TakingExamId" data-id="@Model.ExamId"> Duration : @Model.ExamDuration</p>
    </div>
    <div class="col-mid-6 text-center">
        <h2>@Model.OrganizationName</h2>
        <h3>@Model.CourseName</h3>
        <h4>@Model.ExamTopic</h4>
    </div>
    <div class="col-mid-3">
        <p class="text-right"> Mark : @Model.FullMark</p>
    </div>

</div>
<div id="PartialLoadQuenstionAndAnswerPv">
    
</div>
@section Scripts {

    <script src="~/Scripts/RunnignExam/RunnigExamForParticipant.js"></script>
}